	<command:details>
		<command:name>Invoke-Powerdeploy</command:name>
		<maml:description>
			<maml:para>Deploys a package to a computer.</maml:para>
		</maml:description>
		<maml:copyright>
			<maml:para />
		</maml:copyright>
		<command:verb>Publish</command:verb>
		<command:noun>Package</command:noun>
		<dev:version />
